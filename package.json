    "e2e": "ng e2e",
    "watch": "ng build --watch",
    "dev:ssr": "ng run salsoftfront:serve-ssr",
    "serve:ssr": "node dist/salsoftfront/server/main.js",
    "build:ssr": "ng build --configuration production && ng run salsoftfront:server:production",
    "prerender": "ng run salsoftfront:prerender"
